<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:Navigation_WPF.Views"
                    xmlns:vm="clr-namespace:Navigation_WPF.ViewModels">
    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:HomeView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CustomerViewModel}">
        <view:CustomerView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ProductViewModel}">
        <view:ProductView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OrderViewModel}">
        <view:OrderView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TransactionViewModel}">
        <view:TransactionView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ShipmentViewModel}">
        <view:ShipmentView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SettingViewModel}">
        <view:SettingView/>
    </DataTemplate>
</ResourceDictionary>